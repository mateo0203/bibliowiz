{
    "manifest_version": 3,
    "name": "bibliowiz",
    "version": "1.0",
    "description": "Google Chrome extension used to display references on the body of Research Papers",
    "icons": {
        "128": "src/images/wizard-128.png"
      },
      "action": {
        "default_popup": "src/popups/test.html"
      },
      "permissions": [
              "activeTab"
            ],
      "content_scripts": [
        {
          "js": ["dist/content.js"],
          "matches": [
            "<all_urls>"
          ]
        }
      ]
    }