{
    "manifest_version": 3,
    "name": "bibliowiz",
    "version": "1.0",
    "description": "Google Chrome extension used to display references on the body of Research Papers",
    "icons": {
        "128": "images/wizard-128.png"
      },
      "action": {
        "default_popup": "test.html"
      },
      "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://developer.chrome.com/docs/extensions/*",
            "https://developer.chrome.com/docs/webstore/*"
          ]
        }
      ]
    }